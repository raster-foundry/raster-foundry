{
  "name": "raster-foundry",
  "version": "0.0.1",
  "description": " ",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --config config/webpack.prod.config.js --bail --progress --profile",
    "server": "webpack-dev-server --config config/webpack.dev.config.js --history-api-fallback --inline --progress",
    "serve-static": "npm run build && serve --path dist --port 9001",
    "test": "karma start config/karma.conf.js",
    "test-watch": "karma start config/karma.conf.js --auto-watch --no-single-run",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/azavea/raster-foundry.git"
  },
  "author": "Azavea",
